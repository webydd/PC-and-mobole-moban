
html,
body {
  height: 100%;
  width: 100%;
}
.clearboth {
  height: 0px;
  clear: both;
}
.twb_header {
  height: 105px;
  background: #4d5a72;
}
.twb_header img {
  margin-left: 1.67%;
  width: 172px;
  height: 105px;
  float: left;
}
.twb_header ul {
  margin-left: 6.5%;
  float: left;
  margin-top: 27px;
}
.twb_header ul li {
  margin-right: 15px;
  float: left;
  width: 67px;
  text-align: center;
}
.twb_header ul li a {
  font-size: 18px;
  color: #ffffff;
  height: 54px;
  line-height: 54px;
  display: block;
}
.twb_header ul li a:hover {
  color: #2ea1f9;
  border-bottom: 1px solid #2ea1f9;
}
.twb_header .information {
  width: 300px;
  height: 56px;
  float: right;
  margin-right: 20px;
  margin-left: 21.7%;
  margin-top: 26px;
}
.twb_header .information .xiaoxi1 {
  margin-right: 32px;
  width: 104px;
  float: left;
  font-size: 18px;
  color: #ffffff;
  height: 56px;
  line-height: 56px;
}
.twb_header .information .xiaoxi1 img {
  width: 28px;
  height: 22px;
  margin-top: 17px;
}
.twb_header .information .passer {
  float: left;
  width: 160px;
  color: #ffffff;
  height: 56px;
  line-height: 56px;
}
.twb_header .information .passer span {
  height: 56px;
  margin-bottom: -21px;
  color: #ffffff;
}
.twb_header .information .passer .touxiang {
  width: 48px;
  height: 56px;
  display: inline-block;
  margin-left: auto;
  margin-right: auto;
  overflow: hidden;
  border-radius: 50%;
}
.banner {
  min-height: 554px;
  background: #ffffff;
  width: 100%;
}
.banner .banner_left {
  width: 17.5%;
  min-height: 554px;
  background: red;
  float: left;
  margin-top: 21px;
}
.banner .banner_left li {
  height: 60px;
  line-height: 60px;
  color: #ffffff;
  font-size: 20px;
  background-image: url(../img/ps1/jiantou1.png) no-repeat center right;
  background-color: #0181e3;
}
.banner .banner_left li img {
  width: 60px;
  float: left;
}
.banner .banner_left li span {
  height: 60px;
  display: block;
  text-indent: 20%;
}
.banner .banner_right {
  width: 79.5%;
  min-height: 528px;
  border: 21px solid #f1f1f1;
  float: left;
  border-bottom: 29px solid #f1f1f1;
}
.banner .banner_right .twb_add {
  float: right;
  margin-right: 53px;
  margin-top: 27px;
  margin-bottom: 20px;
  border: 1px solid #e5e5e5;
  border-radius: 5px;
  line-height: 36px;
  width: 135px;
  height: 36px;
  font-size: 14px;
  color: #3dadec;
  text-align: center;
}
.tab6 {
  width: 97%;
  margin-left: 1.5%;
  height: 272px;
  border: 1px solid #e5e5e5;
  border-spacing: 0px;
}
.head {
  height: 46px;
  background: #e5e5e5;
  font: 20px/46px "宋体";
  color: #000000;
}
.td2 {
  text-align: center;
}
tbody tr {
  border-bottom: 1px solid #e5e5e5;
}
.footer {
  height: 80px;
  background: #4d5a72;
  width: 100%;
}

